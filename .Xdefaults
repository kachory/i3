!!! ROFI Color theme !!!

! Base
rofi.fullscreen: true
rofi.padding: 200
rofi.bw: 0
! Use extended color scheme
rofi.color-enabled:                  true
! Color scheme for normal row
rofi.color-normal:                   argb:00000000,  #F0E8CE , argb:00000000,  #deaa87 ,  #1B2B34
! Color scheme for urgent row
rofi.color-urgent:                   argb:00000000,  #F0E8CE , argb:00000000,  #deaa87 ,  #1B2B34
! Color scheme for active row
rofi.color-active:                   argb:00000000,  #9CE9FF , argb:00000000,  #deaa87 ,  #1B2B34
! Color scheme window
rofi.color-window:                   argb:ee222222,  #F0E8CE ,  #FAC863
! Separator type
rofi.separator-style:                solid


!!! URxvt-unicode !!!

! Terminal
URxvt.termName: xterm-256color

! Fake Transparency
!URxvt.transparent: true
!URxvt.shading: 15

! Font
URxvt.font: xft:Monaco-9
URxvt.letterSpace: -1

! Scroll
URxvt*scrollBar: false

! Basic Settings
URxvt*internalBorder: 5
URxvt*cursorBlink: true
URxvt*savelines: 10000
!URxvt*cursorUnderline: true

! Perl extensions
URxvt*perl-ext:
URxvt*perl-ext-common:
URxvt.perl-ext-common: default,clipboard,matcher
URxvt.url-launcher: /usr/bin/chromium
URxvt.matcher.button: 1
URxvt.colorBD: white
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.iso14755: False

! Colorscheme
! Thèmes à garder en tête : 
! Eighties +++
! Grayscale Dark ++
! Material +
! Tomorrow Night ++++

! Base16 Tomorrow Night
! Scheme: Chris Kempson (http://chriskempson.com)

#define base00 #1d1f21
#define base01 #282a2e
#define base02 #373b41
#define base03 #969896
#define base04 #b4b7b4
#define base05 #c5c8c6
#define base06 #e0e0e0
#define base07 #ffffff
!#define base08 #dca3a3
#define base08 #cc6666
#define base09 #de935f
#define base0A #f0c674
#define base0B #b5bd68
#define base0C #8abeb7
#define base0D #81a2be
#define base0E #b294bb
#define base0F #a3685a

*foreground:   base05
#ifdef background_opacity
*background:   [background_opacity]base00
#else
*background:   base00
#endif
*cursorColor:  base05

*color0:       base00
*color1:       base08
*color2:       base0B
*color3:       base0A
*color4:       base0D
*color5:       base0E
*color6:       base0C
*color7:       base05

*color8:       base03
*color9:       base08
*color10:      base0B
*color11:      base0A
*color12:      base0D
*color13:      base0E
*color14:      base0C
*color15:      base07

! Note: colors beyond 15 might not be loaded (e.g., xterm, urxvt),
! use 'shell' template to set these if necessary
*color16:      base09
*color17:      base0F
*color18:      base01
*color19:      base02
*color20:      base04
*color21:      base06

